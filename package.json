{
  "name": "@makerxstudio/node-common",
  "version": "0.1.2",
  "private": false,
  "description": "A set of MakerX core NodeJS types and utilities",
  "author": "MakerX",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:0-clean": "rimraf dist coverage",
    "build:1-compile": "tsc -p tsconfig.build.json",
    "build:2-copy-pkg-json": "npx --yes @makerx/ts-toolkit@latest copy-package-json",
    "test": "jest --coverage --coverageReporters=text",
    "npm:link": "npm link ./dist",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "audit": "npm audit"
  },
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.omit": "^4.5.7",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-date": "^1.1.6",
    "jest-junit": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
